{
  "name": "web3-jsonrpc-gateway",
  "version": "1.4.0",
  "description": "A Web3 provider that handles accounts on its own but delegates chain queries to a 3rd party service",
  "main": "src/bin/jsonrpc.js",
  "bin": {
    "w3gw-ethers": "dist/src/bin/ethers/",
    "w3gw-boba": "dist/src/bin/ethers/",
    "w3gw-celo": "dist/src/bin/ethers/celo.js",
    "w3gw-conflux": "dist/src/bin/conflux/",
    "w3gw-infura": "dist/src/bin/ethers/infura.js"
  },
  "scripts": {
    "lint": "prettier-standard --format",
    "build": "tsc",
    "test": "jest",
    "ethereum:rinkeby": "node dist/src/bin/ethers/infura 8544 rinkeby",
    "ethereum:ropsten": "node dist/src/bin/ethers/infura 8543 ropsten",
    "ethereum:goerli": "node dist/src/bin/ethers/infura 8545 goerli",
    "ethereum:kovan": "node dist/src/bin/ethers/infura 8542 kovan",
    "ethereum:mainnet": "node dist/src/bin/ethers/infura 9545 homestead",
    "conflux:testnet": "node dist/src/bin/conflux https://portal-test.confluxrpc.com 1 8540",
    "boba:rinkeby": "cross-env-shell DEFAULT_GAS_PRICE=15000000 ESTIMATE_GAS_LIMIT=false node dist/src/bin/ethers https://rinkeby.boba.network 8539",
    "boba:mainnet": "cross-env-shell DEFAULT_GAS_PRICE=15000000 ESTIMATE_GAS_LIMIT=false node dist/src/bin/ethers https://mainnet.boba.network 9539"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/witnet/web3-jsonrpc-gateway.git"
  },
  "keywords": [
    "ethereum",
    "json-rpc",
    "middleware",
    "web3",
    "wallet",
    "gateway"
  ],
  "author": "Witnet Foundation and contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/witnet/web3-jsonrpc-gateway/issues"
  },
  "homepage": "https://github.com/witnet/web3-jsonrpc-gateway#readme",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^15.6.1",
    "dotenv": "^10.0.0",
    "jest": "^27.0.1",
    "prettier-standard": "^16.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@celo-tools/celo-ethers-wrapper": "^0.0.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "ethers": "^5.2.0",
    "express": "^4.17.1",
    "js-conflux-sdk": "^1.7.0",
    "json-rpc-2.0": "^0.2.16",
    "winston": "^3.3.3"
  }
}
