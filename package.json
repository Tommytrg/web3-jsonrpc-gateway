{
  "name": "web3-jsonrpc-gateway",
  "version": "1.2.1",
  "description": "A Web3 provider that handles accounts on its own but delegates chain queries to a 3rd party service",
  "main": "src/bin/jsonrpc.js",
  "bin": {
    "w3gw-ethers": "dist/src/bin/ethers/",
    "w3gw-infura": "dist/src/bin/ethers/infura.js",
    "w3gw-conflux": "dist/src/bin/conflux/"
  },
  "scripts": {
    "lint": "prettier-standard --format",
    "build": "tsc",
    "test": "jest",
    "testnets": "npm run testnets:ethereum && npm run conflux:testnet && npm run omgx.rinkeby",
    "testnets:ethereum": "npm run ethereum:rinkeby && npm run ethereum:ropsten && npm run ethereum:goerli && npm run ethereum:kovan",
    "ethereum:rinkeby": "start /b node ./dist/src/bin/ethers/infura 8544 rinkeby >> logs\\eth.rinkeby.log",
    "ethereum:ropsten": "start /b node ./dist/src/bin/ethers/infura 8543 ropsten >> logs\\eth.ropsten.log",
    "ethereum:goerli": "start /b node ./dist/src/bin/ethers/infura 8545 goerli >> logs\\eth.goerli.log",
    "ethereum:kovan": "start /b node ./dist/src/bin/ethers/infura 8542 kovan >> logs\\eth.kovan.log",
    "ethereum:mainnet": "start /b node ./dist/src/bin/ethers/infura 9545 homestead >> logs\\eth.homestead.log",
    "conflux:testnet": "start /b node ./dist/src/bin/conflux https://portal-test.confluxrpc.com 1 8540 >> logs\\cfx.testnet.log",
    "boba:rinkeby": "set DEFAULT_GAS_PRICE=15000000 && start /b node ./dist/src/bin/ethers https://rinkeby.boba.network 8539 >> logs\\boba.rinkeby.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/witnet/web3-jsonrpc-gateway.git"
  },
  "keywords": [
    "ethereum",
    "json-rpc",
    "middleware",
    "web3",
    "wallet",
    "gateway"
  ],
  "author": "Witnet Foundation and contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/witnet/web3-jsonrpc-gateway/issues"
  },
  "homepage": "https://github.com/witnet/web3-jsonrpc-gateway#readme",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^15.6.1",
    "dotenv": "^10.0.0",
    "jest": "^27.0.1",
    "prettier-standard": "^16.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "ethers": "^5.2.0",
    "express": "^4.17.1",
    "js-conflux-sdk": "^1.6.3",
    "json-rpc-2.0": "^0.2.16",
    "winston": "^3.3.3"
  }
}
